{
  "name": "dpts",
  "djangoAppName": "website",
  "version": "1.0.1",
  "description": "Brunch with TypeScript & SASS",
  "scripts": {
    "start": "brunch watch --server",
    "prod": "brunch build --production",
    "build": "npm run prod && venv/bin/python manage.py collectstatic --ignore *.map --noinput",
    "build-win": "npm run prod && \"venv\\Scripts\\python.exe\" manage.py collectstatic --ignore *.map --noinput"
  },
  "devDependencies": {
    "brunch": "^2.5.3",
    "brunch-typescript": "^1.8.1",
    "sass-brunch": "^2.6.3",
    "uglify-js-brunch": "^2.0.1"
  }
}
